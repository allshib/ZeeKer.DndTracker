    <DetailView Id="StorageOperation_DetailView">
      <Layout Id="Layout">
        <LayoutGroup Id="Main">
          <LayoutGroup Id="SimpleEditors">
            <LayoutGroup Id="StorageOperation">
              <LayoutGroup Id="StorageOperation_col1">
