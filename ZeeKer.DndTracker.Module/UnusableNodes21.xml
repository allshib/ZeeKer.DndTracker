      <Layout Id="Layout">
        <LayoutGroup Id="Main">
          <TabbedGroup Id="Tabs">
            <LayoutGroup Id="Transitions" TextAlignMode="AlignLocal">
              <LayoutItem Id="Transitions" TextAlignMode="AutoSize" />
            </LayoutGroup>
            <LayoutGroup Id="Appearances" TextAlignMode="AlignLocal">
              <LayoutItem Id="Appearances" TextAlignMode="AutoSize" />
            </LayoutGroup>
          </TabbedGroup>
        </LayoutGroup>
      </Layout>
    </DetailView>
    <DetailView Id="SelectFeatViewModel_DetailView">
      <Layout Id="Layout">
